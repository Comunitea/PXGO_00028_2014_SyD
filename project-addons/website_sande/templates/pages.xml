<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <!-- Pumping Systems -->
        <record id="pumping_systems_page" model="website.page">
            <field name="name">Pumping Systems</field>
            <field name="website_ids" eval="[(6, 0, [ref('website.default_website')])]"/>
            <field name="menu_ids" eval="[(6, 0, [ref('website_sande.menu_pumping_systems')])]"/>
