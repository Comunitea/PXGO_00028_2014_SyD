                                <div class="col-md-4 col-sm-12 ">
                                    <h2 class="mt0 mb0 join-title text-primary">Join to <t t-esc="website.name"/></h2>
<!--                                    <div class="mt4 ml4 subscribe-title text-secondary">Subscribe to our newsletter</div>-->
                                </div>
                                <div class="col-md-4 col-sm-12 text-center">
                                    <t t-set="is_follower" t-value="website.check_follower(channel)"/>
                                    <div class="subscription_form">
                                        <t t-if="not is_follower" >
                                            <div class="input-group mb8">
                                                <input type="email" name="email" class="form-control" placeholder="your email..."
                                                       t-att-value="user_id.partner_id.email if website.user_id != user_id else None"/>
                                                <span class="input-group-btn">
                                                    <a href="#" class="btn btn-primary js_subscribe_banner_btn" rel="nofollow, noindex">Subscribe</a>
                                                </span>
                                            </div>
                                            <div class="small mb8 legal-terms text-secondary">
                                                With subscribe I accept the <a href="/legal/advice">legal advice</a>, the <a href="/legal/privacy-policy">privacy policy</a> and the <a href="/legal/terms-of-use">terms of use</a> of this website.
                                            </div>
                                        </t>
                                        <t t-else="">
                                            <div class="input-group mb8">
                                                <input type="email" name="email" class="form-control" readonly="readonly"
                                                       t-att-value="is_follower.email"/>
                                                <span class="input-group-btn">
                                                    <a href="#" class="btn btn-warning js_un_subscribe_banner_btn" rel="nofollow, noindex">Unsubscribe</a>
                                                </span>
                                            </div>
                                        </t>
                                    </div>
                                    <div class="alert alert-danger hidden fub_invalid">Invalid e-mail address.</div>
                                    <div class="alert alert-success hidden fub_thanks">Thanks for your subscription!</div>
                                    <div class="alert alert-warning hidden fub_unsubscribed">You have been successfully unsubscribed!</div>
                                </div>
                                <div class="col-md-4 col-sm-12 o_sharing_links">
                                    <a t-att-href="website.social_facebook" t-if="website.social_facebook" target="_blank" title="Facebook"><i class="fa fa-facebook-square o_facebook"/></a>
                                    <a t-att-href="website.social_twitter" t-if="website.social_twitter" target="_blank" title="Twitter"><i class="fa fa-twitter-square o_twitter"/></a>
                                    <a t-att-href="website.social_linkedin" t-if="website.social_linkedin" target="_blank" title="LinkedIn"><i class="fa fa-linkedin-square o_linkedin"/></a>
                                    <a t-att-href="website.social_youtube" t-if="website.social_youtube" target="_blank" title="Youtube"><i class="fa fa-youtube-square o_youtube"/></a>
                                    <a t-att-href="website.social_googleplus" t-if="website.social_googleplus" rel="publisher" target="_blank" title="Google Plus"><i class="fa fa-google-plus-square o_google"/></a>
                                    <a t-att-href="website.social_github" t-if="website.social_github" target="_blank" title="GitHub"><i class="fa fa-github-square o_github"/></a>
                                    <span t-if="hash_tag" t-esc="hash_tag" class="ml4 text-uppercase"/>
                                </div>
                            </div>
                        </div>
                    </section>
                </div>
            </div>
        </t>
    </template>
</odoo>